{
  "name": "rry_betty_pro",
  "version": "1.0.0",
  "description": "一个基于react+webpack的脚手架项目",
  "main": "index.js",
  "scripts": {
    "start": "cross-env ENV_LWD=development webpack serve  --mode=development --config script/webpack.config.js",
    "build": "cross-env ENV_LWD=production webpack --mode=production --config script/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:tsx": "eslint --ext .tsx src && eslint --ext .ts src",
    "lint:css": "stylelint --aei .less .css src",
    "precommit": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "pre-commit": [
    "precommit",
    "precommit-msg"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LazyTraveller/rry_betty_pro.git"
  },
  "keywords": [
    "pro",
    "react"
  ],
  "author": "heweizhi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LazyTraveller/rry_betty_pro/issues"
  },
  "homepage": "https://github.com/LazyTraveller/rry_betty_pro#readme",
  "dependencies": {
    "antd": "^4.15.1",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-mobx": "^2.0.0",
    "history": "^5.0.0",
    "mobx": "^6.2.0",
    "mobx-react": "^7.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "rry-ui": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-webpack-plugin": "^2.5.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^8.1.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "typescript": "^4.2.4",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
